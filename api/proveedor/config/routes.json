{
  "routes": [
    {
      "method": "GET",
      "path": "/proveedors",
      "handler": "proveedor.find",
      "config": {
        "policies": ["global::Administrator","global::Access"]
      }
    },
    {
      "method": "GET",
      "path": "/proveedors/count",
      "handler": "proveedor.count",
      "config": {
        "policies": ["global::Administrator","global::Access"]
      }
    },
    {
      "method": "GET",
      "path": "/proveedors/:id",
      "handler": "proveedor.findOne",
      "config": {
        "policies": ["global::Administrator","global::Access"]
      }
    },
    {
      "method": "POST",
      "path": "/proveedors",
      "handler": "proveedor.create",
      "config": {
        "policies": ["global::Administrator","global::Access"]
      }
    },
    {
      "method": "PUT",
      "path": "/proveedors/:id",
      "handler": "proveedor.update",
      "config": {
        "policies": ["global::Administrator","global::Access"]
      }
    },
    {
      "method": "DELETE",
      "path": "/proveedors/:id",
      "handler": "proveedor.delete",
      "config": {
        "policies": ["global::Administrator","global::Access"]
      }
    }
  ]
}
