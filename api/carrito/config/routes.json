{
  "routes": [
    {
      "method": "GET",
      "path": "/carritos",
      "handler": "carrito.find",
      "config": {
        "policies": ["global::Administrator","global::Access"]
      }
    },
    {
      "method": "GET",
      "path": "/carritos/count",
      "handler": "carrito.count",
      "config": {
        "policies": ["global::Administrator","global::Access"]
      }
    },
    {
      "method": "GET",
      "path": "/carritos/:id",
      "handler": "carrito.findOne",
      "config": {
        "policies": ["global::Administrator","global::Access"]
      }
    },
    {
      "method": "POST",
      "path": "/carritos",
      "handler": "carrito.create",
      "config": {
        "policies": ["global::Administrator","global::Access"]
      }
    },
    {
      "method": "PUT",
      "path": "/carritos/:id",
      "handler": "carrito.update",
      "config": {
        "policies": ["global::Administrator","global::Access"]
      }
    },
    {
      "method": "DELETE",
      "path": "/carritos/:id",
      "handler": "carrito.delete",
      "config": {
        "policies": ["global::Administrator","global::Access"]
      }
    },
    {
      "method": "POST",
      "path": "/carritos/table/files",
      "handler": "carritos.file",
      "config": {
        "policies": []
      }
    }
  ]
}
